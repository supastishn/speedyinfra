{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "node/no-unsupported-features/es-syntax": ["error", { "version": ">=20.0.0" }],
    "prettier/prettier": "error"
  }
}
